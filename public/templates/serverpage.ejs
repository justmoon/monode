	<span class="type">Service</span>
	<span class="name"><%=service.type == 5 ? 'System' : service.name %></span>
  </div>
  <div class="body" style="display: none">
<% if (service.type == 3) { %>
	<p>test</p>
	<p>test</p>
	<p>test</p>
	<p>test</p>
	<p>test</p>
<% } else if (service.type == 5) { %>
	
<% } else { %>
	<p>Error: Service type not yet supported.</p>
<% } %>
  </div>
</div>
<% } %>
