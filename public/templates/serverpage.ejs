  <dd class="uptime"></dd>
  <dt>Monit ID</dt>
  <dd class="monit_id"><%=server.id%></dd>
  <dt>Kernel</dt>
  <dd class="monit_id"><%=server.platform.name%> <%=server.platform.release%></dd>
</dt>
<%
for (var i in server.service) {
	var service = server.service[i];
%>
